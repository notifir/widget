{
  "name": "wc-notification-bell",
  "type": "module",
  "version": "0.1.3",
  "keywords": [
    "webcomponents",
    "lit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/titenkov/wc-notification-bell"
  },
  "main": "dist/index.es.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": "./dist/index.es.js"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "analyze": "custom-elements-manifest analyze --globs 'src/index.ts' --outdir dist",
    "dev": "vite",
    "build": "tsc && vite build && npm run copy:demo && npm run docs:gen",
    "copy:demo": "cp demo/* dist/",
    "preview": "vite preview",
    "docs:gen": "npm run analyze && esmo scripts/gen-docs.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo-elements/lit-apollo": "^5.0.0",
    "@apollo/client": "^3.6.6",
    "lit": "^2.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.21.1",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@custom-elements-manifest/to-markdown": "^0.1.0",
    "@types/node": "^17.0.30",
    "eslint": "^8.14.0",
    "esno": "^0.14.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "typescript": "^4.6.4",
    "vite": "^2.9.6"
  },
  "lint-staged": {
    "*": "eslint"
  },
  "customElements": "dist/custom-elements.json"
}
